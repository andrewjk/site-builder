    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.2.0",
    "electron": "^5.0.6",
    "electron-builder": "^20.44.4",
    "electron-webpack": "^2.7.2",
    "eslint": "^6.0.1",
